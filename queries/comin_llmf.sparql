##
## run with:
## slm-run --load data/publications.rdf --config config.ini -f queries/comin_llmf.sparql --debug

PREFIX dct: <http://purl.org/dc/terms/>
PREFIX ex: <http://example.org/>
        
SELECT ?game
 WHERE {
    ?pub dct:isPartOf ?part .
    ?part dc:title ?journal .
    BIND(CONCAT("""In the scientific world, give me the impact 
      factor or CORE ranking as JSON for the journal or 
      conference entitled : """,STR(?journal)) as ?prompt)
    BIND (ex:SLM-LLM(?prompt) as ?llm)
} limit 10 
